{
  "$schema": "https://github.com/hyperledger-labs/fablo/releases/download/2.2.0/schema.json",
  "global": {
    "fabricVersion": "2.5.9",
    "tls": false,
    "engine": "docker",
    "peerDevMode": false
  },
  "orgs": [
    {
      "organization": {
        "name": "Orderer",
        "domain": "orderer.example.com"
      },
      "orderers": [
        {
          "groupName": "group1",
          "type": "solo",
          "instances": 1
        }
      ],
      "tools": {}
    },
    {
      "organization": {
        "name": "Org1",
        "domain": "org1.example.com"
      },
      "peer": {
        "instances": 2,
        "db": "LevelDb"
      },
      "tools": {}
    },
    {
      "organization": {
        "name": "Org2",
        "domain": "org2.example.com"
      },
      "peer": {
        "instances": 2,
        "db": "LevelDb"
      },
      "tools": {}
    }
  ],
  "channels": [
    {
      "name": "mychannel",
      "orgs": [
        {
          "name": "Org1",
          "peers": [
            "peer0",
            "peer1"
          ]
        },
        {
          "name": "Org2",
          "peers": [
            "peer0",
            "peer1"
          ]
        }
      ]
    }
  ],
  "chaincodes": [
    {
      "name": "asset",
      "version": "1.0",
      "lang": "golang",
      "channel": "mychannel",
      "directory": "./chaincode/asset",
      "endorsement": "OR('Org1MSP.member', 'Org2MSP.member')"
    }
  ]
}